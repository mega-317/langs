package practice;

public class Exam07 {
	public static void main(String[] args) {
		Car myCar1 = new Car("빨강", 0);
		Car myCar2 = new Car("파랑", 0);
		Car myCar3 = new Car("초록", 0);
		
		System.out.println("생산된 차의 대수 : " + Car.carCount);
		System.out.println("생산된 차의 대수 : " + Car.currentCarCount());
		System.out.println("차의 최고 속도 제한 : " + Car.MAXSPEED);
		
		System.out.println("PI의 값 " + Math.PI);
		System.out.println("3의 5제곱 값 " + Math.pow(3, 5));
	}
}
