package practice;

public class Car {
	String color;
	int speed = 0;
	
	static int carCount = 0;
	final static int MAXSPEED = 200;
	final static int MINSPEED = 0;
	
	static int currentCarCount() {
		return carCount;
	}
	
	Car (String color, int speed) {
		this.color = color;
		this.speed = speed;
		carCount ++;
	}
}
